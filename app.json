{
  "expo": {
    "name": "Eurovision Bingo",
    "slug": "eurovision_bingo",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "eurovision-bingo-scheme",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.eurobingo.client",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "The app does not ask for or need to use your microphone to work.",
        "ITSAppUsesNonExemptEncryption": "False"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aagb1884.eurovision_bingo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "dbf9ddb5-3341-4566-ac43-3fec7b349524"
      }
    }
  }
}
